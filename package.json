{
  "name": "react-native-demo",
  "version": "1.0.0",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "prepare": "husky install .husky",
    "preinstall": "npx only-allow pnpm",
    "install-ios-deps": "bundle install && cd ios && bundle exec pod install && cd ..",
    "start": "expo prebuild && watchman watch-del-all && expo start",
    "build:android:debug": "bash ./scripts/build.sh android debug",
    "build:ios:debug": "bash ./scripts/build.sh ios Debug",
    "build:android:release": "bash ./scripts/build.sh android release",
    "build:ios:release": "bash ./scripts/build.sh ios Release",
    "lint": "eslint .",
    "test": "jest",
    "type-check": "tsc",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{md,json}": "prettier --write"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.9",
    "@react-navigation/drawer": "^6.6.4",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@react-navigation/stack": "^6.3.18",
    "expo": "~49.0.13",
    "expo-dev-client": "~2.4.11",
    "expo-screen-orientation": "~6.0.5",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "jotai": "^2.4.3",
    "lottie-react-native": "5.1.6",
    "qss": "^3.0.0",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-mmkv": "^2.10.2",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "^13.14.0",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-webview": "13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^7.0.4",
    "jest": "^29.2.1",
    "lint-staged": "^12.3.4",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-iconfont-cli": "^2.2.4",
    "react-test-renderer": "18.2.0",
    "typescript": "5.1.3"
  },
  "private": true
}
